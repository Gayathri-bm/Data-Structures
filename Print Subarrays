#include<iostream>
using namespace std;
int SubArrays(int arr[], int n ){
for(int start = 0; start <n ; start++){
for(int end =start ; end = end<n ; end ++){
// cout<< "(" << start <<", "<< end<< ")" <<endl;To check the expected result 
for(int i=start; i <=end; i++){
cout<<arr[i];
}
cout<<endl;
}
cout<<endl;
}
int main(){
int arr [5]={5,7,4,9,6,2};
int n = sizeof(arr)/sizeof(int);
int SubArrays(arr, n);
return 0;
}
